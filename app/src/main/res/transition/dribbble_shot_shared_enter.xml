<?xml version="1.0" encoding="utf-8"?>
<transitionSet xmlns:android="http://schemas.android.com/apk/res/android"
    android:interpolator="@android:interpolator/fast_out_slow_in"
    android:transitionOrdering="together">
    <transitionSet>
        <changeBounds android:duration="300"/>
        <transition
            class="com.example.administrator.look.widget.LiftOff"
            android:duration="400"
            android:elevation="6dp"/>
        <targets>
            <target android:targetId="@+id/container"/>
        </targets>
    </transitionSet>


    <transition
        class="com.example.administrator.look.widget.ShotSharedEnter"
        android:duration="300">
        <targets>
            <target android:targetId="@id/shot" />
        </targets>
    </transition>


</transitionSet>